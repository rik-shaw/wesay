<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461</TargetFrameworks>
    <RootNamespace>WeSay.UI</RootNamespace>
    <AssemblyName>WeSay.UI</AssemblyName>
    <Configurations>Debug;Release</Configurations>
    <Description>WeSay WeSay.UI</Description>
    <Company>SIL International</Company>
    <Authors>SIL International</Authors>
    <Product>WeSay</Product>
    <Copyright>Copyright © 2010-2020 SIL International</Copyright>
    <OutputPath>../../output/$(Configuration)</OutputPath>
    <WarningsAsErrors>NU1605;CS8002</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SIL.Core" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.Core.Desktop" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.DictionaryServices" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.Lift" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.Media" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.Windows.Forms" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.Windows.Forms.Keyboarding" Version="8.0.0-beta0258" />
    <PackageReference Include="SIL.WritingSystems" Version="8.0.0-beta0258" />
    <Reference Include="Geckofx-Core">
      <HintPath>..\..\output\$(Configuration)\Geckofx-Core.dll</HintPath>
    </Reference>
    <Reference Include="Geckofx-Winforms">
      <HintPath>..\..\output\$(Configuration)\Geckofx-Winforms.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\LexicalModel\LexicalModel.csproj" />
    <ProjectReference Include="..\WeSay.Foundation\WeSay.Foundation.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Resources\delete-word.png" />
    <None Include="Resources\delete-word-bw.png" />
    <None Include="Resources\delete-word-grey.png" />
    <None Include="Resources\right-arrow.png" />
  </ItemGroup>

</Project>
